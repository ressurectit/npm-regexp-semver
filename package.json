{
    "name": "npm-regexp-semver",
    "version": "1.0.0",
    "description": "NodeJs module that allows updating versions using semver found in files using regexp",
    "main": "index.js",
    "bin":
    {
        "nrs": "bin/npm-regexp-semver.js" 
    },
    "scripts":
    {
        "prebuild": "typings install && gulp copy-tstypings",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./index.js ./*.map ./*.log ./bin",
        "clean:deps": "rimraf ./node_modules ./typings ./tsTypings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run build",
        "postpublish": "npm run clean",
        "republish": "npm unpublish --force && npm publish",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "keywords":
    [
        "npm",
        "semver",
        "regexp"
    ],
    "author": "kukjevov",
    "license": "ISC",
    "dependencies":
    {
        "command-line-args": "~2.1.6",
        "semver": "~5.1.0",
        "fs-finder": "~1.8.1"
    },
    "devDependencies":
    {
        "typescript": "1.8.10",
        "typings": "~0.8.1",
        "rimraf": "~2.5.2",
        "ng2-tstypings": "1.0.5",
        "gulp": "~3.9.1",
        "through2": "~2.0.1",
        "npm-publish-package": "1.2.0"
    }
}